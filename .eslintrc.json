{
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "plugins": ["svelte3"],
  "extends": ["@ls-age", "prettier"],
  "rules": {
    "compat/compat": "off"
  },
  "overrides": [
    {
      "files": ["*.svelte"],
      "processor": "svelte3/svelte3",
      "rules": {
        "prefer-const": "off",
        "@typescript-eslint/indent": "off",
        "no-undef-init": "off",
        "import/imports-first": "off",
        "import/order": "off",
        "import/no-mutable-exports": "off",
        "import/prefer-default-export": "off",
        "import/no-duplicates": "off"
      }
    }
  ]
}
