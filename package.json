{
  "name": "@atscm/docs",
  "version": "0.0.0",
  "description": "Documentation for atscm",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atSCM/docs.git"
  },
  "bugs": {
    "url": "https://github.com/atSCM/docs/issues"
  },
  "homepage": "https://github.com/atSCM/docs#readme",
  "author": "Lukas Hechenberger <lukas.hechenberger@bachmann.info>",
  "keywords": [
    "atscm",
    "docs",
    "documentation",
    "reference",
    "api"
  ],
  "scripts": {
    "build": "sapper build --legacy",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run-junit": "npm run cy:run -- --reporter junit --reporter-options 'mochaFile=test/report/report.xml'",
    "dev": "sapper dev",
    "export": "sapper export --legacy",
    "fetch-data": "node -r esm ./scripts/fetch-data.js",
    "format": "prettier \"**/*\" !.gitignore !.eslintignore \"!{static,__sapper__,test/report,src/data,docs/assets}/**/*\" --write",
    "lint": "eslint --ignore-path .gitignore --ext .js,.svelte .",
    "start": "node __sapper__/build",
    "start:export": "npx serve __sapper__/export -l 3000",
    "test": "run-p --race dev cy:run",
    "test:ci": "run-p --race start:export cy:run-junit"
  },
  "renovate": {
    "extends": [
      "@ls-age:automergeDev"
    ]
  },
  "dependencies": {
    "@polka/send-type": "0.5.2",
    "compression": "1.7.4",
    "fs-extra": "8.1.0",
    "fuzzy-search": "3.0.2",
    "highlight.js": "9.15.10",
    "html-to-text": "5.1.1",
    "htmlslice": "0.1.1",
    "lunr": "2.3.8",
    "marked": "0.7.0",
    "polka": "0.5.2",
    "sirv": "0.4.2",
    "slugify": "1.3.5"
  },
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/plugin-syntax-dynamic-import": "7.7.4",
    "@babel/plugin-transform-runtime": "7.7.6",
    "@babel/preset-env": "7.7.7",
    "@babel/runtime": "7.7.7",
    "@ls-age/eslint-config": "0.9.2",
    "@types/fs-extra": "8.0.1",
    "@types/html-to-text": "1.4.31",
    "bulma": "0.8.0",
    "cypress": "3.8.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-svelte3": "2.7.3",
    "esm": "3.2.25",
    "getopts": "2.2.5",
    "gittar": "0.1.1",
    "npm-run-all": "4.1.5",
    "package-json": "6.5.0",
    "prettier": "1.19.1",
    "prettier-plugin-svelte": "0.7.0",
    "rollup": "1.27.13",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-svelte": "5.1.1",
    "rollup-plugin-terser": "5.1.3",
    "sapper": "0.27.9",
    "semver": "7.1.1",
    "svelte": "3.16.5"
  }
}
