{
  "name": "@atscm/docs",
  "version": "0.0.0",
  "private": true,
  "description": "Documentation for atscm",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atSCM/docs.git"
  },
  "author": "Lukas Hechenberger <lukas.hechenberger@bachmann.info>",
  "homepage": "https://github.com/atSCM/docs#readme",
  "bugs": {
    "url": "https://github.com/atSCM/docs/issues"
  },
  "scripts": {
    "build": "sapper build --legacy",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run-junit": "npm run cy:run -- --reporter junit --reporter-options 'mochaFile=test/report/report.xml'",
    "dev": "sapper dev",
    "export": "sapper export --legacy --entry \"/ /from-cli/?version=export\"",
    "fetch-data": "sucrase-node ./scripts/fetch-data.ts",
    "format": "prettier . \"**/*.svelte\" --ignore-path .gitignore",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts .",
    "start": "node __sapper__/build",
    "start:export": "npx serve __sapper__/export -l 3000",
    "test": "run-p --race dev cy:run",
    "test:ci": "run-p --race start:export cy:run-junit",
    "validate": "svelte-check --ignore src/node_modules/@sapper"
  },
  "keywords": [
    "atscm",
    "docs",
    "documentation",
    "reference",
    "api"
  ],
  "dependencies": {
    "@polka/send-type": "0.5.2",
    "compression": "1.7.4",
    "fs-extra": "8.1.0",
    "fuzzy-search": "3.0.2",
    "highlight.js": "9.15.10",
    "html-to-text": "5.1.1",
    "htmlslice": "0.1.1",
    "lunr": "2.3.8",
    "marked": "0.7.0",
    "polka": "next",
    "sirv": "^1.0.0",
    "slugify": "1.3.5"
  },
  "devDependencies": {
    "@babel/core": "7.12.16",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.12.15",
    "@babel/preset-env": "7.12.16",
    "@babel/runtime": "7.12.13",
    "@ls-age/eslint-config": "0.9.2",
    "@rollup/plugin-babel": "5.2.3",
    "@rollup/plugin-commonjs": "17.1.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "8.4.0",
    "@rollup/plugin-replace": "2.3.4",
    "@rollup/plugin-typescript": "8.1.1",
    "@rollup/plugin-url": "5.0.1",
    "@tsconfig/svelte": "^1.0.10",
    "@types/compression": "^1.7.0",
    "@types/fs-extra": "8.1.0",
    "@types/fuzzy-search": "2.1.0",
    "@types/html-to-text": "5.1.2",
    "@types/marked": "1.2.2",
    "@types/node": "^14.11.1",
    "@types/polka": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "4.15.0",
    "@typescript-eslint/parser": "4.15.0",
    "bulma": "0.9.0",
    "cypress": "4.5.0",
    "eslint": "7.19.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-svelte3": "3.0.0",
    "getopts": "2.2.5",
    "gittar": "0.1.1",
    "npm-run-all": "4.1.5",
    "package-json": "6.5.0",
    "prettier": "2.2.1",
    "prettier-plugin-package": "1.3.0",
    "rollup": "2.36.2",
    "rollup-plugin-svelte": "7.1.0",
    "rollup-plugin-terser": "7.0.2",
    "sapper": "0.29.1",
    "semver": "7.3.4",
    "sucrase": "3.17.1",
    "svelte": "3.32.3",
    "svelte-check": "1.1.34",
    "svelte-preprocess": "4.6.8",
    "tslib": "2.1.0",
    "typescript": "4.1.5"
  },
  "renovate": {
    "extends": [
      "@ls-age:automergeDev"
    ]
  }
}
